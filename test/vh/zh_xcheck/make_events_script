make -j 7 -f Makefile.zh \
	  channel=mmtt \
	  ntuple_path=mmtt \
	  analyzer=AnalyzeMMTT.py \
	  leading_z_selector=zh_zmm_selection.py \
	  primd=DoubleMu \
	  l1name=t1 \
	  l2name=t2 \
	  branches="m1_m2_Mass t1_t2_Mass" \
	  events
